<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.briup.bigdata.pre.grms.web.dao.ext.IResultDAO">
    <select id="findAllResults" resultType="com.briup.bigdata.pre.grms.web.bean.Result">
        select *
        from grms.results r
        <if test="uid!=null">
            <where>
                r.uid = #{uid,jdbcType=INTEGER}
            </where>
            order by r.exp desc
        </if>
    </select>
</mapper>
